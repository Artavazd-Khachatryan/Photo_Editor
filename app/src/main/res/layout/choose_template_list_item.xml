<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">
<data>

    <variable
            name="model"
            type="com.editor.photo.photoeditor.data.models.TemplateModel" />

    <variable
            name="holder"
            type="com.editor.photo.photoeditor.view.adapter.ChooseTemplateListAdapter.ChooseTemplateListViewHolder" />

</data>

    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
            android:layout_margin="8dp">

        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/template_list_item2"
                android:onClick="@{holder::onClick}"/>

</FrameLayout>
</layout>